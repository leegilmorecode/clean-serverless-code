{
  "name": "onion-sounds",
  "version": "0.1.0",
  "bin": {
    "onion-sounds": "bin/onion-sounds.js"
  },
  "scripts": {
    "docs": "npx typedoc",
    "test": "jest --maxWorkers=50% --noStackTrace --silent",
    "test:coverage": "jest --maxWorkers=50% --coverage --silent",
    "test:watch": "jest --watch --maxWorkers=25% --noStackTrace --silent",
    "ls": "cdk ls",
    "cdk": "cdk",
    "diff": "cdk diff",
    "clear:jest": "jest --clearCache",
    "deploy": "cdk deploy --outputs-file ./cdk-outputs.json --all",
    "deploy:hot": "cdk deploy --outputs-file ./cdk-outputs.json --all --hotswap",
    "remove": "cdk destroy --all",
    "deploy:stateful": "cdk deploy StatefulStack --outputs-file ./cdk-outputs.json",
    "remove:stateful": "cdk destroy StatefulStack",
    "deploy:stateless": "cdk deploy StatelessStack --outputs-file ./cdk-outputs.json",
    "remove:stateless": "cdk destroy StatelessStack"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.108",
    "@types/convict": "^6.1.1",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@types/uuid": "^8.3.4",
    "aws-cdk": "2.47.0",
    "esbuild": "^0.15.12",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "aws-cdk-lib": "2.47.0",
    "aws-sdk": "^2.1242.0",
    "constructs": "^10.0.0",
    "convict": "^6.2.3",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
